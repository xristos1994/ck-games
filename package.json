{
  "name": "christos-korompokis-games",
  "author": "Christos Korompokis <xristos.korompokis@gmail.com>",
  "private": true,
  "description": "Some games for friends",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "type-check": "tsc --noEmit",
    "build:type-check": "npm run type-check && npm run build",
    "type-check:watch": "npm run type-check -- --watch",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "lazy:create": "node lazy_dev/scripts/create.js",
    "docker:build:prod": "docker build -f Dockerfile.prod -t ck-games:prod .",
    "docker:run:prod": "docker run -it --rm -p 9000:80 ck-games:prod",
    "docker:build": "docker build -t ck-games .",
    "docker:run": "docker run -it -p 8000:8000 ck-games",
    "docker:compose:up": "docker-compose up"
  },
  "dependencies": {
    "fs-extra": "^9.1.0",
    "gatsby": "^3.0.0",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-manifest": "^3.0.0",
    "gatsby-plugin-offline": "^4.0.0",
    "gatsby-plugin-polyfill-io": "^1.1.0",
    "gatsby-plugin-react-helmet": "^4.0.0",
    "gatsby-plugin-react-svg": "^3.0.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.2.0",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-observable": "^1.2.0",
    "reselect": "^4.0.0",
    "rxjs": "^6.6.3",
    "typescript": "^4.2.3",
    "uifx": "^2.0.7"
  },
  "devDependencies": {
    "prettier": "2.2.1",
    "prompts": "^2.4.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xristos1994/ck-games"
  },
  "bugs": {
    "url": "https://github.com/xristos1994/ck-games/issues"
  }
}
