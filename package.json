{
  "name": "christos-korompokis-games",
  "author": "Christos Korompokis <xristos.korompokis@gmail.com>",
  "private": true,
  "description": "Some games for friends",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "start": "gatsby develop",
    "clean": "gatsby clean",
    "build": "gatsby build",
    "build:prod": "npm run type-check && npm run lint && npm run test && npm run build",
    "serve": "gatsby serve",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test:cover": "npm test -- --coverage",
    "test:cover:watch": "npm test -- --watchAll --coverage",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx",
    "lint:fix": "npm run lint --fix",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,css,json,md}\"",
    "format:lint": "npm run prettier && npm run lint -- --fix",
    "docker:build": "docker build -t ck-games .",
    "docker:run": "docker run -it -p 8000:8000 ck-games",
    "docker:build:prod": "docker build -f Dockerfile.prod -t ck-games:prod .",
    "docker:run:prod": "docker run -it --rm -p 9000:80 ck-games:prod",
    "docker:compose:up": "docker-compose up"
  },
  "dependencies": {
    "fs-extra": "^9.1.0",
    "gatsby": "^3.10.0",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-manifest": "^3.0.0",
    "gatsby-plugin-offline": "^4.0.0",
    "gatsby-plugin-polyfill-io": "^1.1.0",
    "gatsby-plugin-react-helmet": "^4.0.0",
    "gatsby-plugin-react-svg": "^3.0.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.2.0",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-observable": "^1.2.0",
    "reselect": "^4.0.0",
    "rxjs": "^6.6.3",
    "uifx": "^2.0.7"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/react-dom": "^17.0.8",
    "@types/react-helmet": "^6.1.1",
    "@types/react-redux": "^7.1.16",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "babel-jest": "^27.0.6",
    "babel-preset-gatsby": "^1.8.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.25",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "react-test-renderer": "^17.0.2",
    "ts-jest": "^27.0.3",
    "typescript": "^4.2.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xristos1994/ck-games"
  },
  "bugs": {
    "url": "https://github.com/xristos1994/ck-games/issues"
  }
}
